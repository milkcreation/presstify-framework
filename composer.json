{
  "name": "presstify/framework",
  "description": "Framework PresstiFy",
  "keywords": [
    "framework",
    "presstify",
    "wordpress"
  ],
  "homepage": "https://svn.tigreblanc.fr/presstify-framework",
  "time": "2018-11-22",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy.manner@milkcreation.fr",
      "role": "Lead Developper"
    },
    {
      "name": "Julien Picard",
      "email": "julien@tigreblanc.fr",
      "role": "Contributing Developper"
    },
    {
      "name": "Louis Dherent",
      "email": "louis@tigreblanc.fr",
      "role": "Contributing Developper"
    },
    {
      "name": "Gaetan Thery",
      "email": "gaetan@tigreblanc.fr",
      "role": "Contributing Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://composer.tigreblanc.fr/presstify/"
    }
  ],
  "require": {
    "composer/composer": "^1.6",
    "composer/installers": "~1.0",
    "defuse/php-encryption": "^2.0",
    "egeloen/google-map": "^1.0",
    "embed/embed": "^3.3",
    "facebook/graph-sdk": "^5.5",
    "google/apiclient": "^2.2",
    "google/recaptcha": "^1.1",
    "guzzlehttp/guzzle": "^6.2",
    "halilim/xml-iterator": "^1.0",
    "html2text/html2text": "^4.0",
    "illuminate/http": "^5.6",
    "illuminate/support": "^5.6",
    "kenwheeler/slick": "^1.6",
    "league/booboo": "^2.0",
    "league/commonmark": "^0.11",
    "league/container": "^2.4",
    "league/csv": "^9.0",
    "league/event": "^2.1",
    "league/flysystem": "^1.0",
    "league/glide": "^1.3",
    "league/period": "^3.4",
    "league/plates": "^3.3",
    "league/route": "^3.0",
    "league/uri": "^5.2",
    "madcoda/php-youtube-api": "^1.2",
    "mexitek/phpcolors": "^0.4.0",
    "mobiledetect/mobiledetectlib": "^2.8",
    "monolog/monolog": "^1.22",
    "mrclay/minify": "^3.0",
    "pelago/emogrifier": "^2.0",
    "phpdocumentor/reflection-docblock": "^3.2.2",
    "phpmailer/phpmailer": "^5.2",
    "redeyeventures/geopattern": "^1.1",
    "symfony/filesystem": "^3.3",
    "symfony/finder": "^4.0",
    "symfony/http-foundation": "^4.0",
    "symfony/psr-http-message-bridge": "^1.0",
    "symfony/serializer": "^4.0",
    "symfony/yaml": "^4.0",
    "twbs/bootstrap": "^3.3",
    "vimeo/vimeo-api": "^1.2",
    "zendframework/zend-diactoros": "^1.7"
  },
  "suggest": {
    "beelab/bowerphp": "Utilisation de bower via PHP",
    "bshaffer/oauth2-server-php": "Serveur OAuth 2.0",
    "codeitnowin/barcode": "Bibliothèque de création de code barre dynamique",
    "dompdf/dompdf": "Convertion de HTML en PDf",
    "endroid/qr-code": "Bibliothèque de création de QR Code",
    "FortAwesome/Font-Awesome": "Ancienne gestion intégré de FontAwesome, presstiFy utilise maintenant Bower",
    "mpratt/embera": "Bibliothèque client d'encapsulation de vidéo",
    "neitanod/forceutf8": "Forçage d'encodage en utf-8 d'une chaine de caractères"
  },
  "autoload": {
    "classmap": ["tiFy.php"],
    "files": [
      "bin/helpers/Components.php",
      "bin/helpers/Core.php",
      "bin/helpers/Lib.php",
      "src/helpers.php"
    ],
    "psr-4": {
      "tiFy\\": ["bin/", "src/"],
      "tiFy\\App\\": "bin/app/",
      "tiFy\\Components\\": "components/",
      "tiFy\\Core\\": "core/",
      "tiFy\\Deprecated\\": "bin/deprecated/",
      "tiFy\\Environment\\" : "bin/deprecated/env/",
      "tiFy\\Maintenance\\" : "bin/maintenance/",
      "tiFy\\Set\\": "set/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "scripts": {
    "post-install-cmd": [
      "bower install --allow-root --save"
    ],
    "post-update-cmd": [
      "bower install --allow-root --save"
    ]
  }
}
