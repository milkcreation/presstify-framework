{
  "name": "presstify/framework",
  "description": "Framework PresstiFy",
  "keywords": [
    "framework",
    "presstify",
    "wordpress"
  ],
  "homepage": "https://svn.tigreblanc.fr/presstify-framework",
  "time": "2020-08-06",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy.manner@milkcreation.fr",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.tigreblanc.fr/presstify"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "composer/composer": "^1.6",
    "composer/installers": "~1.0",
    "doctrine/dbal": "^2.9",
    "embed/embed": "^4.2",
    "facebook/graph-sdk": "^5.5",
    "google/apiclient": "^2.2",
    "google/recaptcha": "^1.1",
    "guzzlehttp/guzzle": "^6.5",
    "guzzlehttp/psr7": "^1.6",
    "html2text/html2text": "^4.0",
    "illuminate/http": "^5.8",
    "illuminate/database": "^5.8",
    "jgrossi/corcel": "^2.8",
    "laminas/laminas-httphandlerrunner": "^1.1",
    "laminas/laminas-diactoros": "^2.3",
    "league/container": "^2.4",
    "league/event": "^2.1",
    "league/flysystem": "^1.0",
    "league/flysystem-cached-adapter": "^1.0",
    "league/period": "^4.7",
    "league/plates": "^3.3",
    "league/route": "^4.4",
    "league/uri": "^6.2",
    "league/uri-components": "^2.2",
    "lhsazevedo/restatic": "^1.0",
    "madcoda/php-youtube-api": "^1.2",
    "matthiasmullie/minify": "^1.3",
    "mobiledetect/mobiledetectlib": "^2.8",
    "monolog/monolog": "^2.1",
    "nyholm/psr7": "^1.3",
    "pelago/emogrifier": "^3.1",
    "phpmailer/phpmailer": "^6.0",
    "ralouphie/mimey": "^2.1",
    "ramsey/uuid": "^4.0",
    "respect/validation": "^2.0",
    "symfony/console": "^4.2",
    "symfony/dom-crawler": "^5.1",
    "symfony/filesystem": "^4.2",
    "symfony/finder": "^4.2",
    "symfony/framework-bundle": "^4.2",
    "symfony/http-foundation": "^4.2",
    "symfony/lock": "^4.2",
    "symfony/psr-http-message-bridge": "^2.0",
    "symfony/security-csrf": "^4.2",
    "symfony/serializer": "^4.2"
  },
  "suggest": {
    "bshaffer/oauth2-server-php": "Serveur OAuth 2.0",
    "codeitnowin/barcode": "Bibliothèque de création de code barre dynamique",
    "dompdf/dompdf": "Convertion de HTML en PDf",
    "egeloen/google-map": "Gestion Google Map (deprecated)",
    "endroid/qr-code": "Bibliothèque de création de QR Code",
    "filp/whoops": "Librairie de traitement des erreurs PHP",
    "intervention/image": "Librairie de manipulation des images",
    "league/commonmark": "Markdown parser conforme à la specification CommonMark",
    "mexitek/phpcolors": "Librairie de gestion des couleurs avec PHP",
    "mpratt/embera": "Bibliothèque client d'encapsulation de vidéo",
    "mrclay/minify": "Minification avancée des scripts CSS et JS",
    "mustache/mustache": "Gestionnaire de templates PHP",
    "neitanod/forceutf8": "Forçage d'encodage en utf-8 d'une chaine de caractères",
    "phpdocumentor/reflection-docblock": "Analyse des annotation PHP DocBlock",
    "redeyeventures/geopattern": "Librairie de génération de motif géométrique",
    "swiftmailer/swiftmailer": "Librairie d'expédition de mail by Symfony",
    "twig/twig": "Librairie de Template by Symfony",
    "vimeo/vimeo-api": "Client PHP Api Vimeo",
    "wp-cli/media-command": "Commandes WP Cli des médias (dépréciation composer 2.0)",
    "wp-cli/search-replace-command": "Commandes WP Cli de base de données (dépréciation composer 2.0)",
    "wp-cli/wp-cli": "Commandes WP Cli (dépréciation composer 2.0)"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "tiFy\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "scripts": {
    "post-install-cmd": [
      "bower install --allow-root --save"
    ],
    "post-update-cmd": [
      "bower install --allow-root --save"
    ]
  }
}
