{
  "name": "presstify/debug",
  "description": "Debug PresstiFy Component",
  "homepage": "https://svn.tigreblanc.fr/presstify/debug/",
  "keywords": [
    "presstify",
    "components",
    "debug"
  ],
  "type": "presstify-component",
  "time": "2020-11-27",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy.manner@milkcreation.fr",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.tigreblanc.fr/presstify/"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "maximebf/debugbar": "^1.16",
    "presstify/framework": ">=1.4",
    "symfony/error-handler": "^5.2",
    "filp/whoops": "^2.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "tiFy\\Debug\\": ""
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
