{
    "name": "presstify/framework",
    "description": "Framework PresstiFy",
    "keywords": [
        "framework",
        "presstify",
        "wordpress"
    ],
    "homepage": "https://svn.tigreblanc.fr/presstify-framework/",
    "time": "2020-11-09",
    "authors": [
        {
            "name": "Jordy Manner",
            "email": "jordy.manner@milkcreation.fr",
            "role": "Developper"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.tigreblanc.fr/presstify/"
        }
    ],
    "require": {
        "monolog/monolog": "^1.22",
        "league/csv": "^8.2",
        "defuse/php-encryption": "^2.0",
        "pelago/emogrifier": "^1.2",
        "symfony/yaml": "^3.2",
        "phpmailer/phpmailer": "^5.2",
        "mobiledetect/mobiledetectlib": "^2.8",
        "redeyeventures/geopattern": "^1.1",
        "html2text/html2text": "^4.0",
        "google/recaptcha": "^1.1",
        "bacon/bacon-qr-code": "^1.0",
        "mexitek/phpcolors": "^0.4.0",
        "facebook/graph-sdk": "^5.5",
        "guzzlehttp/guzzle": "^6.2",
        "bshaffer/oauth2-server-php": "^1.9",
        "mrclay/minify": "^3.0",
        "twbs/bootstrap": "^3.3",
        "FortAwesome/Font-Awesome": "^4.7",
        "kenwheeler/slick": "^1.6",
        "pear/http2": "^1.1",
        "neitanod/forceutf8": "^2.0",
        "league/commonmark": "^0.11",
        "madcoda/php-youtube-api": "^1.2",
        "vimeo/vimeo-api": "^1.2",
        "mpratt/embera": "^1.9",
        "egeloen/google-map": "^1.0",
        "google/apiclient": "^2.2",
        "dompdf/dompdf": "^0.8.0",
        "phpdocumentor/reflection-docblock": "^3.2.2",
        "codeitnowin/barcode": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "tiFy\\": ["", "./bin/deprecated"],
            "tiFy\\Abstracts\\": ["./bin/lib/Abstracts", "./bin/deprecated/lib/Abstracts"],
            "tiFy\\App\\": "./bin/app",
            "tiFy\\Core\\": "./core",
            "tiFy\\Components\\": "./components",
            "tiFy\\Lib\\Checker\\": "./bin/lib/Checker",
            "tiFy\\Lib\\Country\\": "./bin/lib/Country",
            "tiFy\\Lib\\Facebook\\": "./bin/lib/Facebook",
            "tiFy\\Lib\\File\\": "./bin/lib/File",
            "tiFy\\Lib\\QrCode\\": "./bin/lib/QrCode",
            "tiFy\\Lib\\StdClass\\": "./bin/lib/StdClass",
            "tiFy\\Lib\\": ["./bin/lib", "./bin/deprecated/lib"],
            "tiFy\\Inherits\\": ["./bin/lib/Inherits"],
            "tiFy\\Environment\\": "./bin/deprecated/env",
            "tiFy\\Plugins\\": "./plugins",
            "tiFy\\Set\\": "./set",
            "tiFy\\Statics\\": "./bin/lib/Statics"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
